{
  "name": "demo-blog",
  "version": "1.0.0",
  "description": "",
  "main": ".eleventy.config.js",
  "scripts": {
    "clean": "rimraf './dist'",
    "styles:prod": "sass --no-source-map --style=compressed src/assets/scss/main.sass dist/assets/css/main.css",
    "styles:dev": "sass --source-map --embed-source-map --source-map-urls=absolute ./src/assets/scss/main.sass dist/assets/css/main.css",
    "poststyles:prod": "postcss dist/css/main.css --replace --no-map --use autoprefixer cssnano",
    "scripts:prod": "esbuild main.bundle=./src/assets/js/main.js app.bundle=./src/assets/js/app.js --target=es6 --bundle --minify --outdir=./dist/assets/js/",
    "scripts:dev": "esbuild main.bundle=./src/assets/js/main.js app.bundle=./src/assets/js/app.js --target=es6 --bundle --outdir=./dist/assets/js/",
    "server": "npx @11ty/eleventy --serve --quiet",
    "tina": "npx tinacms dev -c 'npm run dev'",
    "eleventy": "npx @11ty/eleventy --quiet",
    "images": "node \"./build_tasks/transform-images.js\"",
    "watch:scripts": "onchange \"./src/assets/js/**/*\" -- npm run scripts:dev",
    "watch:styles": "onchange \"./src/assets/scss/**/*\" -- npm run styles:dev",
    "build": "npm-run-all clean --parallel images styles:prod scripts:prod eleventy",
    "dev": "npm-run-all --parallel server watch:*",
    "watch": "npm run dev",
    "deploy:aws-examples": "aws s3 sync dist/ s3://examples-11ty.kapuaops.com --delete --no-paginate --no-cli-auto-prompt;  aws cloudfront create-invalidation --distribution-id E367XFRM87X1SZ --paths '/*' --no-cli-auto-prompt --no-paginate --no-cli-pager"
  },
  "author": "thabeat@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-bundle": "^1.0.4",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@parcel/transformer-sass": "^2.9.3",
    "esbuild": "^0.19.2",
    "html-minifier": "^4.0.0",
    "luxon": "^3.4.0",
    "markdown-it-anchor": "^8.6.7",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.4",
    "onchange": "^7.1.0",
    "parcel": "^2.9.3",
    "postcss": "^8.4.27",
    "sass": "^1.65.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@webpixels/css": "^1.2.6",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.1"
  }
}
